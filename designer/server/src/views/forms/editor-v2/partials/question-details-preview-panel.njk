{% from "preview-panel/macro.njk" import appPreviewPanel %}

{% call appPreviewPanel({
    questionType: questionType,
    previewPageUrl: previewPageUrl,
    previewErrorsUrl: previewErrorsUrl,
    errorTemplates: errorTemplates,
    extraFields: extraFields,
    basePageFields: basePageFields,
    model: model,
    skipLink: skipLink
  }) %}
{% endcall %}

<script src="{{ getAssetPath("assets/nunjucks/govuk-components.js") }}"></script>
<script type="module">
  import { setupPreview } from "{{ getAssetPath("preview.js") }}"
  setupPreview('{{ questionType }}')
</script>
