{
  "name": "@defra/forms-designer",
  "version": "3.0.62",
  "private": true,
  "description": "Defra forms designer",
  "homepage": "https://github.com/DEFRA/forms-designer/tree/main/designer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DEFRA/forms-designer.git",
    "directory": "designer"
  },
  "license": "OGL-UK-3.0",
  "type": "module",
  "scripts": {
    "audit": "npm audit --audit-level=high",
    "build": "npm run build:server && npm run build:client",
    "build:client": "NODE_ENV=${NODE_ENV:-production} webpack",
    "build:server": "BABEL_ENV=node babel --delete-dir-on-start --extensions \".js\",\".ts\" --ignore \"**/*.test.*\" --copy-files --no-copy-ignored --source-maps --out-dir ./server/dist ./server/src",
    "predev": "npm run build:server",
    "dev": "concurrently \"npm run client:watch\" \"npm run server:watch\" \"npm run build:server -- --skip-initial-build --watch\"",
    "symlink-env": "./bin/symlink-config",
    "test": "jest --color --coverage --verbose",
    "test:watch": "jest --color --watch",
    "client:watch": "NODE_ENV=development npm run build:client -- --watch",
    "server:watch": "nodemon",
    "prestart": "npm run build",
    "start": "NODE_ENV=${NODE_ENV:-production} node --enable-source-maps ./server/dist/index.js"
  },
  "dependencies": {
    "@defra/forms-model": "^3.0.62",
    "@elastic/ecs-pino-format": "^1.5.0",
    "@hapi/basic": "^7.0.2",
    "@hapi/bell": "^13.0.2",
    "@hapi/boom": "^10.0.1",
    "@hapi/catbox-memory": "^6.0.1",
    "@hapi/catbox-redis": "^7.0.2",
    "@hapi/cookie": "^12.0.1",
    "@hapi/crumb": "^9.0.1",
    "@hapi/hapi": "^21.3.8",
    "@hapi/inert": "^7.1.0",
    "@hapi/jwt": "^3.2.0",
    "@hapi/scooter": "^7.0.0",
    "@hapi/vision": "^7.0.3",
    "@hapi/wreck": "^18.1.0",
    "@hapi/yar": "^11.0.2",
    "@xgovformbuilder/govuk-react-jsx": "^7.1.2",
    "blankie": "^5.0.0",
    "classnames": "^2.5.1",
    "depth-first": "^4.0.0",
    "dotenv": "^16.4.5",
    "focus-trap-react": "^8.11.3",
    "govuk-frontend": "^4.8.0",
    "hapi-pino": "^12.1.0",
    "i18next": "^21.10.0",
    "i18next-http-backend": "^1.4.5",
    "ioredis": "^5.3.2",
    "joi": "^17.13.0",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "nanoid": "^3.3.4",
    "pino": "^8.20.0",
    "prismjs": "^1.29.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.3.4",
    "react-simple-code-editor": "^0.11.3",
    "react-sortable-hoc": "^1.11.0",
    "resolve": "^1.22.8",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.5.2",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^12.0.2",
    "core-js": "^3.37.0",
    "css-loader": "^7.1.1",
    "dagre": "^0.8.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.8",
    "file-loader": "^6.2.0",
    "jsdom": "^24.0.0",
    "mini-css-extract-plugin": "^2.9.0",
    "nunjucks": "^3.2.4",
    "pino-pretty": "^11.0.0",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "sass-embedded": "^1.75.0",
    "sass-loader": "^14.2.1",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "webpack": "^5.91.0",
    "webpack-assets-manifest": "^5.2.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": "^20.9.0",
    "npm": "^10.1.0"
  }
}
