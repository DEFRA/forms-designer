{
  "name": "@defra/forms-designer",
  "version": "3.0.521",
  "private": true,
  "description": "Defra forms designer",
  "homepage": "https://github.com/DEFRA/forms-designer/tree/main/designer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DEFRA/forms-designer.git",
    "directory": "designer"
  },
  "license": "OGL-UK-3.0",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "audit": "npm audit --audit-level=high",
    "build": "npm run build:server && npm run build:client",
    "build:client": "NODE_ENV=${NODE_ENV:-production} webpack",
    "build:server": "babel --delete-dir-on-start --extensions \".js\",\".ts\" --ignore \"**/*.test.*\" --copy-files --no-copy-ignored --source-maps --out-dir ./server/dist ./server/src",
    "dev": "concurrently \"npm run client:watch\" \"npm run server:watch:dev\" --kill-others --names \"client,server\" --prefix-colors \"red.dim,blue.dim\"",
    "dev:debug": "concurrently \"npm run client:watch\" \"npm run server:watch:debug\" --kill-others --names \"client,server\" --prefix-colors \"red.dim,blue.dim\"",
    "symlink-env": "./bin/symlink-config",
    "test": "jest --color --coverage --verbose",
    "test:watch": "jest --color --watch",
    "client:watch": "NODE_ENV=development npm run build:client -- --watch",
    "server:watch": "NODE_ENV=development tsx watch --clear-screen=false --enable-source-maps --env-file=.env --exclude \"**/*.test.*\" --include \"../model/dist/**/!(*.test).js\"",
    "server:watch:dev": "npm run server:watch -- ./server/src/index.ts",
    "server:watch:debug": "npm run server:watch -- --inspect-wait ./server/src/index.ts",
    "precompile": "node precompile-govuk-components.js",
    "prestart": "npm run build",
    "postinstall": "npm run precompile",
    "start": "NODE_ENV=${NODE_ENV:-production} node --enable-source-maps ./server/dist/index.js"
  },
  "dependencies": {
    "@babel/plugin-syntax-import-attributes": "^7.25.6",
    "@babel/preset-react": "^7.26.3",
    "@dagrejs/dagre": "^1.1.4",
    "@defra/forms-engine-plugin": "^0.1.22",
    "@defra/forms-model": "^3.0.521",
    "@defra/hapi-tracing": "^1.26.0",
    "@elastic/ecs-pino-format": "^1.5.0",
    "@hapi/basic": "^7.0.2",
    "@hapi/bell": "^13.1.0",
    "@hapi/boom": "^10.0.1",
    "@hapi/catbox-memory": "^6.0.2",
    "@hapi/catbox-redis": "^7.0.2",
    "@hapi/cookie": "^12.0.1",
    "@hapi/crumb": "^9.0.1",
    "@hapi/hapi": "^21.4.0",
    "@hapi/inert": "^7.1.0",
    "@hapi/jwt": "^3.2.0",
    "@hapi/vision": "^7.0.3",
    "@hapi/wreck": "^18.1.0",
    "@hapi/yar": "^11.0.2",
    "@types/dagre": "^0.7.52",
    "@types/govuk-frontend": "^5.7.0",
    "@types/hapi__cookie": "^12.0.5",
    "@types/hapi__crumb": "^7.3.7",
    "@types/hapi__inert": "^5.2.10",
    "@types/lodash": "^4.17.17",
    "@types/luxon": "^3.6.2",
    "@types/nunjucks": "^3.2.6",
    "@types/prismjs": "^1.26.5",
    "@types/qs": "^6.14.0",
    "@types/react": "^18.3.12",
