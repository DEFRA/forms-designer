{
  "name": "@defra/forms-designer",
  "version": "3.0.324",
  "private": true,
  "description": "Defra forms designer",
  "homepage": "https://github.com/DEFRA/forms-designer/tree/main/designer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DEFRA/forms-designer.git",
    "directory": "designer"
  },
  "license": "OGL-UK-3.0",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "audit": "npm audit --audit-level=high",
    "build": "npm run build:server && npm run build:client",
    "build:client": "NODE_ENV=${NODE_ENV:-production} webpack",
    "build:server": "babel --delete-dir-on-start --extensions \".js\",\".ts\" --ignore \"**/*.test.*\" --copy-files --no-copy-ignored --source-maps --out-dir ./server/dist ./server/src",
    "dev": "concurrently \"npm run client:watch\" \"npm run server:watch:dev\" --kill-others --names \"client,server\" --prefix-colors \"red.dim,blue.dim\"",
    "dev:debug": "concurrently \"npm run client:watch\" \"npm run server:watch:debug\" --kill-others --names \"client,server\" --prefix-colors \"red.dim,blue.dim\"",
    "symlink-env": "./bin/symlink-config",
    "test": "jest --color --coverage --verbose",
    "test:watch": "jest --color --watch",
    "client:watch": "NODE_ENV=development npm run build:client -- --watch",
    "server:watch": "NODE_ENV=development tsx watch --clear-screen=false --enable-source-maps --env-file=.env --exclude \"**/*.test.*\" --include \"../model/dist/**/!(*.test).js\"",
    "server:watch:dev": "npm run server:watch -- ./server/src/index.ts",
    "server:watch:debug": "npm run server:watch -- --inspect-wait ./server/src/index.ts",
    "prestart": "npm run build",
    "start": "NODE_ENV=${NODE_ENV:-production} node --enable-source-maps ./server/dist/index.js"
  },
  "dependencies": {
    "@dagrejs/dagre": "^1.1.4",
    "@defra/forms-model": "^3.0.324",
    "@elastic/ecs-pino-format": "^1.5.0",
    "@hapi/basic": "^7.0.2",
    "@hapi/bell": "^13.0.2",
    "@hapi/boom": "^10.0.1",
    "@hapi/catbox-memory": "^6.0.2",
    "@hapi/catbox-redis": "^7.0.2",
    "@hapi/cookie": "^12.0.1",
    "@hapi/crumb": "^9.0.1",
    "@hapi/hapi": "^21.3.10",
    "@hapi/inert": "^7.1.0",
    "@hapi/jwt": "^3.2.0",
    "@hapi/vision": "^7.0.3",
    "@hapi/wreck": "^18.1.0",
    "@hapi/yar": "^11.0.2",
    "@xgovformbuilder/govuk-react-jsx": "^7.1.2",
    "classnames": "^2.5.1",
    "cssnano": "^7.0.5",
    "cssnano-preset-default": "^7.0.5",
    "date-fns": "^3.6.0",
    "depth-first": "^4.0.0",
    "focus-trap-react": "^10.2.3",
    "govuk-frontend": "^5.6.0",
    "hapi-pino": "^12.1.0",
    "http-status-codes": "^2.3.0",
    "i18next": "^23.14.0",
    "i18next-http-backend": "^2.6.1",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "nanoid": "^5.0.7",
    "nunjucks": "^3.2.4",
    "pino": "^9.3.2",
    "prismjs": "^1.29.0",
    "proxy-agent": "^6.4.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-simple-code-editor": "^0.14.1",
    "resolve": "^1.22.8",
    "serialize-javascript": "^6.0.2"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.5.2",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^12.0.2",
    "core-js": "^3.38.1",
    "file-loader": "^6.2.0",
    "jsdom": "^25.0.0",
    "outdent": "^0.8.0",
    "pino-pretty": "^11.2.2",
    "postcss": "^8.4.44",
    "postcss-loader": "^8.1.1",
    "sass-embedded": "^1.78.0",
    "sass-loader": "^16.0.1",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "tsx": "^4.19.0",
    "webpack": "^5.94.0",
    "webpack-assets-manifest": "^5.2.1",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": "^20.9.0",
    "npm": "^10.1.0"
  }
}
